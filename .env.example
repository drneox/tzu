# TZU - Environment Variables Template
# Copy this file to .env and update with your values

# AI Integration (optional - leave empty if not available)
OPENAI_API_KEY=sk-your-openai-api-key-here
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# JWT Configuration (optional - auto-generated if not specified)
SECRET_KEY=your-secret-key-here-minimum-32-characters

# Environment Configuration
ENVIRONMENT=development
DEBUG=true

# Note: Database configuration is handled automatically by Docker Compose
# No need to configure DATABASE_URL, POSTGRES_* variables manually
